'Application\BrauneDigital\MessageBundle\Entity\ConversationMessage':
    type: entity
    oneToMany:
        sendTo:
            targetEntity: 'Application\BrauneDigital\MessageBundle\Entity\UserHasMessage'
            inversedBy: message
    oneToOne:
        conversation:
            targetEntity: 'Application\BrauneDigital\MessageBundle\Entity\Conversation'
            inversedBy: messages
    manyToOne:
        by:
            targetEntity: User
            inversedBy: sentMessages